#include <stdio.h>
int main() {
    int pages[50], queue[10], n, f, i, j, k = 0, found, faults = 0;
    printf("Enter number of pages and frames:\n");
    scanf("%d%d", &n, &f);
    printf("Enter the page reference string:\n");
    for(i = 0; i < n; i++)
        scanf("%d", &pages[i]);
    // Initialize queue to -1
    for(j = 0; j < f; j++)
        queue[j] = -1;
    for(i = 0; i < n; i++) {
        found = 0;
        for(j = 0; j < f; j++) {
            if(queue[j] == pages[i]) {
                found = 1;
                break;
            }
        }
        if(!found) {
            queue[k % f] = pages[i];
            k++;
            faults++;
        }
    }
    printf("Page Faults = %d\n", faults);
    return 0;
}


....

echo "Enter a number:"
read num
sum=0
n=$num
while [ $n -gt 0 ]
do
  digit=$((n % 10))
  sum=$((sum + digit * digit * digit))
  n=$((n / 10))
done
if [ $sum -eq $num ]; then
  echo "$num is an Armstrong number."
else
  echo "$num is not an Armstrong number."
fi
