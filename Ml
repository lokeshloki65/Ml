from sklearn.linear_model import LinearRegression

X = [[1, 1], [2, 2], [3, 3]]
y = [2, 4, 6]

model = LinearRegression()
model.fit(X, y)

print("Coefficients:", model.coef_)
print("Intercept:", model.intercept_)
print("Prediction:", model.predict([[4, 4]]))







....


text = input("Enter a sentence: ")
words = text.split()
words.sort()

print("Sorted words:")
for w in words:
    print(w)
